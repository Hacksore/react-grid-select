{
  "name": "react-grid-select",
  "version": "0.1.0",
  "private": true,
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/user-event": "^12.1.10",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest --watchAll=false",
    "start": "start-storybook -p 6006 -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": 0
    }
  },
  "browserslist": {
    "production": [
